                "fn": "insertAndReplace",
                "designer|params": [
                    "locObj",
                    "targetString"
                ],
                "implHandler": [
                    "var newId=\"#\"+Ext.id()+\"#\";\r",
                    "var finalId=Ext.clone(newId);\r",
                    "Ext.Object.each(locObj, function(key, value, myself) {\r",
                    "    if (value==targetString){\r",
                    "        finalId=key;\r",
                    "    }\r",
                    "});\r",
                    "if (newId==finalId){\r",
                    "    locObj[finalId]=targetString;\r",
                    "}\r",
                    "return(finalId);"
                ]
