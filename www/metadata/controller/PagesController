                    "        metaBox.show();\r",
                    "\r",
                    "    },this,{single:true});\r",
                    "        Ext.getCmp(\"mainPageTree\").getSelectionModel().getLastSelected().collapseChildren(true);\r",
                    "        Ext.getCmp(\"mainPageTree\").getStore().load({\"node\":Ext.getCmp(\"mainPageTree\").getSelectionModel().getLastSelected()});\r",
